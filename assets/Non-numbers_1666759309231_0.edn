{:highlights [{:id #uuid "6358ba93-1920-477d-9947-ad588a3b3262", :page 1, :position {:bounding {:x1 90.828125, :y1 87.34375, :x2 297.34844970703125, :y2 118.34375, :width 916, :height 1296.2194996786002}, :rects ({:x1 90.828125, :y1 87.34375, :x2 297.34844970703125, :y2 118.34375, :width 916, :height 1296.2194996786002}), :page 1}, :content {:text "\"Non-numbers\""}, :properties {:color "yellow"}} {:id #uuid "6358baaa-52a5-4fb7-941b-d701640f1328", :page 1, :position {:bounding {:x1 0, :y1 0, :x2 496.4161376953125, :y2 204.859375, :width 916, :height 1296.2194996786002}, :rects ({:x1 0, :y1 0, :x2 0, :y2 0, :width 916, :height 1296.2194996786002} {:x1 90.828125, :y1 184.859375, :x2 496.4161376953125, :y2 204.859375, :width 916, :height 1296.2194996786002}), :page 1}, :content {:text "It is worth restating: everything is a number. "}, :properties {:color "yellow"}} {:id #uuid "6358bac2-e4cf-431f-b97b-9dbfb18948e0", :page 1, :position {:bounding {:x1 0, :y1 0, :x2 753.5042114257812, :y2 255.6875, :width 916, :height 1296.2194996786002}, :rects ({:x1 0, :y1 0, :x2 0, :y2 0, :width 916, :height 1296.2194996786002} {:x1 0, :y1 0, :x2 0, :y2 16, :width 916, :height 1296.2194996786002} {:x1 496.3988952636719, :y1 184.859375, :x2 715.9033203125, :y2 204.859375, :width 916, :height 1296.2194996786002} {:x1 90.828125, :y1 210.265625, :x2 753.5042114257812, :y2 230.265625, :width 916, :height 1296.2194996786002} {:x1 90.828125, :y1 235.6875, :x2 210.90037536621094, :y2 255.6875, :width 916, :height 1296.2194996786002}), :page 1}, :content {:text "This rule is fundamental to understanding how computers work and is one of the most important concepts in programming. "}, :properties {:color "blue"}} {:id #uuid "6358bb54-a691-4722-b6a5-ba7e3ca7936e", :page 1, :position {:bounding {:x1 0, :y1 0, :x2 779.184326171875, :y2 482.109375, :width 916, :height 1296.2194996786002}, :rects ({:x1 0, :y1 0, :x2 0, :y2 16, :width 916, :height 1296.2194996786002} {:x1 90.828125, :y1 435.546875, :x